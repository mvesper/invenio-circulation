{{#entities}}
    <div class="panel panel-default" id="loan_cycle_{{id}}">
        <div class="panel-heading">
            <div class="row">
                <div class="col-md-10">
                    <a href="/circulation/entities/loan_cycle/{{id}}">{{id}}</a>
                </div>
                <div class="col-md-2">
                    <button type="button" class="btn btn-danger btn-block entity_delete" id="delete_{{id}}">Delete</button>
                </div>
            </div>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="col-md-2">Current Status</div>
                <div class="col-md-10">{{current_status}}</div>
            </div>
            <div class="row">
                <div class="col-md-2">Item</div>
                <div class="col-md-10">{{item.id}}</div>
            </div>
            <div class="row">
                <div class="col-md-2">Library</div>
                <div class="col-md-10">{{library}}</div>
            </div>
            <div class="row">
                <div class="col-md-2">User</div>
                <div class="col-md-10">{{user.id}}</div>
            </div>
            <div class="row">
                <div class="col-md-2">Start Date</div>
                <div class="col-md-10">{{start_date}}</div>
            </div>
            <div class="row">
                <div class="col-md-2">End Date</div>
                <div class="col-md-10">{{end_date}}</div>
            </div>
            <div class="row">
                <div class="col-md-2">Date Issued</div>
                <div class="col-md-10">{{date_issued}}</div>
            </div>
        </div>
    </div>
{{/entities}}
