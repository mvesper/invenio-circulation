[{"electronic_location_and_access": [{"electronic_name": ["neil.calder@cern.ch"], "access_method": "Email"}], "publication_distribution_imprint": [{"date_of_publication_distribution": ["14 06 2000"]}], "subject_added_entry_topical_term": [{"topical_term_or_geographic_name_entry_element": "Experiments and Tracks", "level_of_subject": "Primary", "thesaurus": "Source specified in subfield $2", "source_of_heading_or_term": "SzGeCERN"}], "physical_medium": [{"material_base_and_configuration": ["FILM"]}], "main_entry_personal_name": {"personal_name": "Photolab"}, "title_statement": {"title": "ALEPH experiment: Candidate of Higgs boson production"}, "source_of_acquisition": [{"stock_number": "CERN-EX-0106015"}], "varying_form_of_title": [{"type_of_title": "Parallel title", "title_proper_short_title": "Exp\u00e9rience ALEPH: Candidat de la production d'un boson Higgs"}], "index_term_uncontrolled": [{"uncontrolled_term": ["LEP"], "level_of_index_term": "Primary"}], "summary": [{"summary": "Candidate for the associated production of the Higgs boson and Z boson. Both, the Higgs and Z boson decay into 2 jets each. The green and the yellow jets belong to the Higgs boson. They represent the fragmentation of a bottom andanti-bottom quark. The red and the blue jets stem from the decay of the Z boson into a quark anti-quark pair. Left: View of the event along the beam axis. Bottom right: Zoom around the interaction point at the centre showing detailsof the fragmentation of the bottom and anti-bottom quarks. As expected for b quarks, in each jet the decay of a long-lived B meson is visible. Top right: \"World map\" showing the spatial distribution of the jets in the event."}]}, {"electronic_location_and_access": [{"electronic_name": ["marie.noelle.pages.ribeiro@cern.ch"], "access_method": "Email"}], "publication_distribution_imprint": [{"date_of_publication_distribution": ["10 Apr 2001"]}], "subject_added_entry_topical_term": [{"topical_term_or_geographic_name_entry_element": "Experi